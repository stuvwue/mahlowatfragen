{
	"theses": {
		"0": {
			"l": "Der*Die Bürgermeister*in soll sich dagegen einsetzen, den Klimanotstand für Würzburg auszurufen.",
			"s": "Kein Klimanotstand",
			"x": "Hier wird Klimanotstand als die Festlegung verstanden, dass der Stadtrat bei allen Entscheidungen die Auswirkungen auf den Ausstoß von klimawirksamen Substanzen berücksichtigen muss.."
		},
		"1": {
			"l": "Eine Wahlliste für den Stadtrat soll nur zugelassen werden, wenn sie ein gleichmäßiges Geschlechterverhältnis aufweist.",
			"s": "Geschlechterquotierung für Wahllisten",
			"x": "Es geht hier um die grundsätzliche Einstellung, nicht um die jetzt erfolgenden Wahlen!"
		},
		"2": {
			"l": "Die Würzburger Innenstadt soll weitestgehend mit dem PKW befahrbar sein.",
			"s": "Keine autofreie Innenstadt",
			"x": ""
		},
		"3": {
			"l": "Bus- und Radverkehr sollen keine Bevorzugung gegenüber PKW im Straßenverkehr erhalten.",
			"s": "Verkehrskonzept I: PKW vor Bus und Rad",
			"x": ""
		},
		"4": {
			"l": "Würzburg braucht mehr Parkplätze.",
			"s": "Verkehrskonzept II: mehr Parkplätze",
			"x": ""
		},
		"5": {
			"l": "Ein Park-and-Ride-Konzept für Würzburg ist nicht zielführend.",
			"s": "Verkehrskonzept III: kein P&R",
			"x": ""
		},
		"6": {
			"l": "Städtische Gebäude sollen soweit wie möglich mit Solarpaneelen ausgestattet werden.",
			"s": "Photovoltaik",
			"x": ""
		},
		"7": {
			"l": "Die Posthalle mit ihrem kulturellen Angebot muss bestehen bleiben.",
			"s": "Posthalle erhalten",
			"x": ""
		},
		"8": {
			"l": "Würzburg braucht kein freies Kulturzentrum.",
			"s": "Kein freies Kulturzentrum",
			"x": ""
		},
		"9": {
			"l": "Die Radwegesituation in Würzburg ist miserabel und muss verbessert werden.",
			"s": "Radwege verbessern",
			"x": ""
		},
		"10": {
			"l": "Das Semesterticket Mainfrankentheater soll zu einem umfänglichen Kulturticket ausgebaut werden.",
			"s": "Kulturticket",
			"x": ""
		},
		"11": {
			"l": "Für die Einführung der Straßenbahn zum Hubland sollen auch Enteignungen in Kauf genommen werden.",
			"s": "Straßenbahnlinie Hubland",
			"x": ""
		},
		"12": {
			"l": "Die Linie 10 soll mindestens bis 15 min nach Bibliotheksschluss befahren werden.",
			"s": "Linie 10",
			"x": "Die Bibliothek ist Werktags bis 24:00 geöffnet."
		},
		"13": {
			"l": "Es gibt ausreichend Nachtbusse in Würzburg.",
			"s": "Nachtbusse ausreichend",
			"x": "Damit sind die 90er-Linien gemeint."
		},
		"14": {
			"l": "Die katholische Kirche soll weiterhin Mitspracherecht bei der Besetzung von bestimmten, nicht theologischen Lehrstühlen besitzen.",
			"s": "Konkordatslehrstühle",
			"x": "An der Uni Wü gibt es drei Lehrstühle, bei deren Besetzung der Bischof Vetos einlegen darf."
		},
		"15": {
			"l": "Es sollen mehr Wohnungen gebaut werden.",
			"s": "Wohnungen bauen",
			"x": ""
		},
		"16": {
			"l": "Eine Deckelung der Mietpreise ist nicht sinnvoll.",
			"s": "Mietpreis deckeln",
			"x": ""
		},
		"17": {
			"l": "Es ist nicht notwendig, bestimmte Wohnungen ausschließlich für Studierende anzubieten.",
			"s": "Wohnraum nicht beschränken",
			"x": "Eine Ausnahme sind selbstverständlich die Wohnheime des Studentenwerks."
		},
		"18": {
			"l": "Die Studierenden machen etwa ein Viertel der Würzburger Bevölkerung aus, sind aber in kommunalpolitischen Entscheidungen nicht ausreichend eingebunden.",
			"s": "Studis in der Politik einbinden",
			"x": ""
		},
		"19": {
			"l": "Die Stadt Würzburg soll den Handel mit Hanfprodukten stärker beschränken.",
			"s": "Cannabis beschränken",
			"x": ""
		}
	},
